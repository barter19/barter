{
  "name": "vuecharged-template",
  "version": "1.0.0",
  "description": "An opinionated Vue 2.0 starter template with CLI scaffolding superpowers.",
  "main": "src/main.js",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- . ",
    "dev": "webpack-dev-server --inline --progress --color --config ./internals/webpack/webpack.dev.config.js",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack --config ./internals/webpack/webpack.prod.config.js --color -p --progress",
    "generate": "plop --plopfile internals/generators/index.js",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "author": "Daniel Escobedo <daniel@escobedo.io>",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:mrboomer/vuecharged-template.git",
    "type": "git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "./src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "container/(.*)$": "components/container/$1",
      "presentational/(.*)$": "components/presentational/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest",
      "\\.(css|scss)$": "<rootDir>/internals/helpers/fileTransformer.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/helpers/fileTransformer.js"
    },
    "snapshotSerializers": [
      "jest-serializer-html"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,vue}"
    ]
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.14.1",
    "eslint-plugin-vue": "^4.2.2",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.2",
    "jest-serializer-html": "^5.0.0",
    "jsdom": "^11.6.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.8.2",
    "plop": "^1.9.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "vue-jest": "^2.1.1",
    "vue-loader": "^14.2.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "circular-dependency-plugin": "^4.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "html-webpack-plugin": "^2.30.1",
    "offline-plugin": "^4.9.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^3.11.0",
    "whatwg-fetch": "^2.0.3"
  }
}
