{
  "name": "vuecharged-template",
  "version": "1.0.0",
  "description": "An opinionated feature-first Vue 2.0 template with CLI scaffolding superpowers.",
  "author": "Daniel Escobedo <daniel@escobedo.io>",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:mrboomer/vuecharged-template.git",
    "type": "git"
  },
  "scripts": {
